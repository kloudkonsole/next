{
	"mod": [{
		"id": "web",
		"mod": "./mod/web",
		"host": "127.0.0.1",
		"port": 1337
	},{
		"id": "db",
		"mod": "./mod/json_mem",
		"format": {
			"pkey": "x",
			"state": "state",
			"created_at": "cat",
			"updated_at": "uat",
			"created_by": "cby",
			"updated_by": "uby"
		}
	},{
		"id": "timer",
		"mod": "./mod/timer"
	},{
		"id": "util",
		"mod": "./mod/util"
	}],
	"rsc": {
		"user": {
			"format": {
			},
			"schema": {
				"type": "object",
				"spec": {
					"name": {
						"type": "array",
						"gt": 0,
						"spec": "string"
					},
					"ccode": "number",
					"mobile": "number",
					"email": "string",
					"address": {
						"type": "array",
						"gt": 0,
						"spec": "string"
					}
				}
			}
		}
	},
	"spec": {
		"grouping": [["index", "csv"], ["range", "start", "end"]],
		"params": {
			"type": "object",
			"spec": {
				"id": "string"
			}
		},
		"query": {
			"type": "object",
			"spec": {
				"page": "number",
				"size": "number",
				"sort": "string",
				"q": "string",
				"filter": {
					"type": "array",
					"sep": "~",
					"spec": {
						"type": "object",
						"spec": {
							"include": {
								"type": "array"
							},
							"range": "string",
							"start": {
								"type": "null",
								"required": ["bool", ["range"]]
							},
							"end": {
								"type": "null",
								"required": ["bool", ["range"]]
							},
							"index": "string",
							"csv": {
								"type": "array",
								"sep": "~",
								"required": ["bool", ["index"]]
							}
						}
					}
				}
			}
		}
	},
	"routes": {
		"* * * * * *": [
			["util.log", "hello"]
		],
		"/1.0/health": [
			["web.log", "_.res"],
			["util.wait", 5],
			[["web.output", "text/plain", "text"], "hello"]
		],
		"/1.0/user/:id": [
			["web.log", "_.res"],
			[["web.input", "$.spec.params"], "$.params", "_.params"],
			[["web.input", "$.spec.query", "$.spec.grouping"], "_.url.query", "_.query"],
			[["web.output"], "_.res", "_.params", "_.query"]
		]
	}
} 
